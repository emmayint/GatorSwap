<!DOCTYPE html>
<html>
    <head>
        <title>GatorSwap</title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
        <link rel="stylesheet" href="/stylesheets/main.css">
        <script src="https://code.jquery.com/jquery-git2.min.js"></script>
    </head>
    <body>
    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header col-md-2">
                <a class="navbar-brand" href="/">GatorSwap</a>
            </div>
            <div class="collapse navbar-collapse">
                <ul class="nav nav-search col-md-7">
                    <li>
                      <form action="/listings" method="GET" class="form-inline">
                      <div class="form-group">
                        <input type="text" name="search" placeholder="try book..." class="form-control">
                        <input type="submit" value="Search" class="btn">
                      </div>
                    </form>
                    </li>
                  </ul>
                  <ul class="nav navbar-nav col-md-3">
                    <% if(!currentUser){ %>
                        <!--</li>-->
                        <li><a href="/login">Login</a></li>
                        <li><a href="/register">Sign Up</a></li>
                    <% } else { %>
                        <li><a href="#">Signed In As <%= currentUser.username %></a></li>
                        <li><a href="/logout">Logout</a></li>
                    <% } %>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container-fluid">
         <a class="btn" href="/listings">Shop Now</a>
         <a class="btn" href="/listings/new">Add New Listing</a>
        <% if(currentUser){ %>
         <a class="btn" href="#">My Dashboard</a>
        <% } %>
        <p></p>
    </div>
    <div class="container-fluid">
       <% if(error && error.length > 0) { %>
         <div class="alert alert-danger"><p><%=error %></p></div>
       <% } %>

       <% if(success && success.length > 0) { %>
         <div class="alert alert-success"><p><%= success %></p></div>
       <% } %>
    </div>
    <div class="container">
      <p>
    </div>
